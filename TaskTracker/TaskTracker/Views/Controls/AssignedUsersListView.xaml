<?xml version="1.0" encoding="UTF-8"?>
<xForms:SfListView xmlns:xForms="clr-namespace:Syncfusion.ListView.XForms;assembly=Syncfusion.SfListView.XForms"
                         xmlns:forms="http://xamarin.com/schemas/2014/forms"
                         IsEnabled="False" Orientation="Horizontal">
    <xForms:SfListView.ItemTemplate>
        <forms:DataTemplate >
            <forms:Frame HeightRequest="30" WidthRequest="30" CornerRadius="15">
                <forms:Image Source="{forms:Binding Image}"/>
            </forms:Frame>
        </forms:DataTemplate>
    </xForms:SfListView.ItemTemplate>
</xForms:SfListView>